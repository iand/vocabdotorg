<?xml version="1.0" encoding="utf-8"?><rdf:RDF xmlns:aiiso="http://purl.org/vocab/aiiso/schema#" xmlns:skos="http://www.w3.org/2004/02/skos/core#" xmlns:cc="http://web.resource.org/cc/" xmlns:dctype="http://purl.org/dc/dcmitype/" xmlns:dct="http://purl.org/dc/terms/" xmlns:vann="http://purl.org/vocab/vann/" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" xmlns:owl="http://www.w3.org/2002/07/owl#" xmlns:xsd="http://www.w3.org/2001/XMLSchema#">	<owl:Ontology rdf:about="" dc:date="2008-05-20">		<dc:title xml:lang="en">Academic Institution Internal Structure Ontology (AIISO)</dc:title>		<rdfs:comment xml:lang="en">			The Academic Institution Internal Structure Ontology (AIISO) provides classes and properties to describe the internal organisational structure of an academic institution.    	</rdfs:comment>		<dc:identifier>http://purl.org/vocab/aiiso/schema-20080520</dc:identifier>		<dct:isVersionOf rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dc:creator>Rob Styles</dc:creator>		<dc:creator>Nadeem Shabir</dc:creator>		<dc:contributor>Ian Corns</dc:contributor>		<dc:contributor>Sarah Bartlett</dc:contributor>		<dc:rights>Copyright Â© 2008 Talis Information Ltd</dc:rights>		<vann:preferredNamespaceUri>http://purl.org/vocab/aiiso/schema#</vann:preferredNamespaceUri>		<vann:preferredNamespacePrefix>aiiso</vann:preferredNamespacePrefix>		<dct:replaces rdf:resource="http://purl.org/vocab/aiiso/schema-20080514"/>		<dct:hasFormat>			<dctype:Text rdf:about="http://purl.org/vocab/aiiso/schema-20080520.html">				<dc:format>					<dct:IMT>						<rdf:value>text/html</rdf:value>						<rdfs:label xml:lang="en">HTML</rdfs:label>					</dct:IMT>				</dc:format>			</dctype:Text>		</dct:hasFormat>		<dct:hasFormat>			<dctype:Text rdf:about="http://purl.org/vocab/aiiso/schema-20080520.rdf">				<dc:format>					<dct:IMT>						<rdf:value>application/rdf+xml</rdf:value>						<rdfs:label xml:lang="en">RDF</rdfs:label>					</dct:IMT>				</dc:format>			</dctype:Text>		</dct:hasFormat>		<dct:issued>2008-05-14</dct:issued>		<skos:historyNote rdf:value="First cut of the ontology published" dc:date="2008-05-14" dc:creator="Rob Styles"/>		<skos:historyNote rdf:value="Corrected classes with more than one possible domain class to use owl:unionOf" dc:date="2008-05-20" dc:creator="Rob Styles"/>		<skos:historyNote rdf:value="Added statement of responsibility for both knowledgeGrouping and organisationalUnit" dc:date="2008-05-20" dc:creator="Nadeem Shabir"/>	</owl:Ontology>	<cc:Work rdf:about="">		<cc:license rdf:resource="http://creativecommons.org/licenses/by/2.0/uk/"/>	</cc:Work>	<cc:License rdf:about="http://creativecommons.org/licenses/by/2.0/uk/">		<cc:permits rdf:resource="http://web.resource.org/cc/Reproduction"/>		<cc:permits rdf:resource="http://web.resource.org/cc/Distribution"/>		<cc:requires rdf:resource="http://web.resource.org/cc/Notice"/>		<cc:requires rdf:resource="http://web.resource.org/cc/Attribution"/>		<cc:permits rdf:resource="http://web.resource.org/cc/DerivativeWorks"/>	</cc:License>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#OrganisationalUnit">		<rdfs:label xml:lang="en">Organisational Unit</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An OrganisationalUnit is used to describe a group of people that form a part of an organisations insitutional hierarchy. More specific classes of Institution, Department, Faculty, School, College, Center, Division and Institute exist and are preferred.</rdfs:comment>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Institution">		<rdfs:label xml:lang="en">Institution</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Insitution is an OrganisationalUnit that represents the upper most level of an academic institution.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Department">		<rdfs:label xml:lang="en">Department</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Department is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as a department.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Faculty">		<rdfs:label xml:lang="en">Faculty</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Faculty is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as a faculty.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#School">		<rdfs:label xml:lang="en">School</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A School is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as a school.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#College">		<rdfs:label xml:lang="en">College</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A College is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as a college.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Center">		<rdfs:label xml:lang="en">Center</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Center is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as a center.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Division">		<rdfs:label xml:lang="en">Division</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Division is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as a division.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Institute">		<rdfs:label xml:lang="en">Institute</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An Institute is an OrganisationalUnit that represents a group of people recognised by an OrganisationalUnit as forming a cohesive group referred to by the organisation as an institute.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping">		<rdfs:label xml:lang="en">KnowledgeGrouping</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A KnowledgeGrouping represents a collection of resources, learning objectives, timetables, and other materials. The more specific subclasses of Subject, Programme, Course and Module are preferred.</rdfs:comment>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Programme">		<rdfs:label xml:lang="en">Programme</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Programme is a KnowledgeGrouping that represents a cohesive collection of educational material referred to by the owning OrganisationalUnit as a programme.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Subject">		<rdfs:label xml:lang="en">Subject</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Subject is a KnowledgeGrouping that represents a cohesive collection of educational material referred to by the owning OrganisationalUnit as a subject.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Course">		<rdfs:label xml:lang="en">Course</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Course is a KnowledgeGrouping that represents a cohesive collection of educational material referred to by the owning OrganisationalUnit as a course.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdfs:Class rdf:about="http://purl.org/vocab/aiiso/schema#Module">		<rdfs:label xml:lang="en">Module</rdfs:label>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Module is a KnowledgeGrouping that represents a cohesive collection of educational material referred to by the owning OrganisationalUnit as a module.</rdfs:comment>		<rdfs:subClassOf rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdfs:Class>	<rdf:Property rdf:about="http://purl.org/vocab/aiiso/schema#name">		<rdfs:label xml:lang="en">name</rdfs:label>		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The name given to a KnowledgeGrouping or OrganisationalUnit.</rdfs:comment>		<rdfs:domain>			<owl:Class>				<owl:unionOf rdf:parseType="Collection">					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>				</owl:unionOf>			</owl:Class>		</rdfs:domain>		<rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#Label"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>		<skos:historyNote rdf:value="Corrected the domain to use an owl:unionOf the two classes, allowing subjects of this property to be either a Knowledge Grouping or an Organisational Unit rather than having to be both." dc:date="2008-05-20" dc:creator="Rob Styles"/>	</rdf:Property>	<rdf:Property rdf:about="http://purl.org/vocab/aiiso/schema#code">		<rdfs:label xml:lang="en">code</rdfs:label>		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The code used by an Institution to refer to a KnowledgeGrouping or OrganisationalUnit.</rdfs:comment>		<rdfs:domain>			<owl:Class>				<owl:unionOf rdf:parseType="Collection">					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>				</owl:unionOf>			</owl:Class>		</rdfs:domain>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>		<skos:historyNote rdf:value="Corrected the domain to use an owl:unionOf the two classes, allowing subjects of this property to be either a Knowledge Grouping or an Organisational Unit rather than having to be both." dc:date="2008-05-20" dc:creator="Rob Styles"/>	</rdf:Property>	<rdf:Property rdf:about="http://purl.org/vocab/aiiso/schema#description">		<rdfs:label xml:lang="en">description</rdfs:label>		<rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A Description of the KnowledgeGrouping or OrganisationalUnit.</rdfs:comment>		<rdfs:domain>			<owl:Class>				<owl:unionOf rdf:parseType="Collection">					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>				</owl:unionOf>			</owl:Class>		</rdfs:domain>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>		<skos:historyNote rdf:value="Corrected the domain to use an owl:unionOf the two classes, allowing subjects of this property to be either a Knowledge Grouping or an Organisational Unit rather than having to be both." dc:date="2008-05-20" dc:creator="Rob Styles"/>	</rdf:Property>	<rdf:Property rdf:about="http://purl.org/vocab/aiiso/schema#organisationalUnit">		<rdfs:label xml:lang="en">organisationalUnit</rdfs:label>		<rdfs:range rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An OrganisationalUnit may contain other OrganisationalUnits using organisationalUnit.</rdfs:comment>		<rdfs:domain rdf:resource="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>	</rdf:Property>	<rdf:Property rdf:about="http://purl.org/vocab/aiiso/schema#knowledgeGrouping">		<rdfs:label xml:lang="en">knowledgeGrouping</rdfs:label>		<rdfs:range rdf:resource="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>		<rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A KnowledgeGrouping may be contained by another KnowledgeGrouping or an OrganisationalUnit using knowledgeGrouping.</rdfs:comment>		<rdfs:domain>			<owl:Class>				<owl:unionOf rdf:parseType="Collection">					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>				</owl:unionOf>			</owl:Class>		</rdfs:domain>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-14</dct:issued>		<skos:historyNote rdf:value="Corrected the domain to use an owl:unionOf the two classes, allowing subjects of this property to be either a Knowledge Grouping or an Organisational Unit rather than having to be both." dc:date="2008-05-20" dc:creator="Rob Styles"/>	</rdf:Property>	<rdf:Property rdf:resource="http://purl.org/vocab/aiiso/schema#responsibilityOf">		<rdfs:label xml:lang="en">Responsibility Of</rdfs:label>		<rdfs:domain>			<owl:Class>				<owl:unionOf rdf:parseType="Collection">					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#KnowledgeGrouping"/>					<owl:Class rdf:about="http://purl.org/vocab/aiiso/schema#OrganisationalUnit"/>				</owl:unionOf>			</owl:Class>		</rdfs:domain>		<rdfs:range rdf:resource="http://xmlns.com/foaf/spec/Agent"/>		<rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/aiiso/schema#"/>		<dct:issued>2008-05-20</dct:issued>	</rdf:Property></rdf:RDF>