            <owl:onProperty rdf:resource="&vocabns;personInSituation"/>
          </owl:Restriction>
        </owl:intersectionOf>
      </owl:Class>
    </owl:equivalentClass>

  </owl:Class>


  <owl:ObjectProperty rdf:about="&vocabns;subject">
